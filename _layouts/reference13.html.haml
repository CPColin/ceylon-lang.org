#core-page
  - page.source_path_rel = Pathname.new(page.source_path).relative_path_from(site.dir)
  - page.view_source_url = "https://github.com/ceylon/ceylon-lang.org/blob/master/#{page.source_path_rel}"
  - page.edit_source_url = "https://github.com/ceylon/ceylon-lang.org/edit/master/#{page.source_path_rel}"
  .gh-edit
    %a{ :href => page.view_source_url }
      View source 
    - if page.gh_editable
      |
      %a{ :href => page.edit_source_url}
        Edit
  - if ["Milestone 1", "Milestone 2", "Milestone 3", "Milestone 4", "Milestone 4"].index(page.milestone)
    .milestone.check
      :markdown
        [#{page.milestone}](/documentation/1.1/roadmap/##{page.milestone.anchorize({:convert_spaces=>true})}_done "Support for this feature was introduced in #{page.milestone}")
  - if ["Milestone 6"].index(page.milestone)
    .milestone.future
      :markdown
        [Ceylon 1.1](/documentation/1.1/roadmap/#milestone_6_ceylon_10 "Support for this feature will be introduced in Ceylon 1.1")
  - if ["Later"].index(page.milestone)
    .milestone.future
      :markdown
        [Not yet implemented](/documentation/1.1/roadmap/#ceylon_11_or_later "Support for this feature will be introduced in Ceylon 1.1 or later")
  ~ content
